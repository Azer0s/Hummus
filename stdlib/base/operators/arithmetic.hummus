(def * (fn elements
  (--system-do-math! :* elements)
))

(def / (fn elements
  (--system-do-math! :/ elements)
))

(def + (fn elements
  (--system-do-math! :+ elements)
))

(def - (fn elements
  (--system-do-math! :- elements)
))