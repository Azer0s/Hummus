(def = (fn first second
  (--system-do-compare! := first second)
))

(def != (fn first second
  (--system-do-compare! :/= first second)
))

(def /= (fn first second
  (--system-do-compare! :/= first second)
))

(def < (fn first second
  (--system-do-compare-arithmetic! :< first second)
))

(def > (fn first second
  (--system-do-compare-arithmetic! :> first second)
))

(def <= (fn first second
  (--system-do-compare-arithmetic! :<= first second)
))

(def >= (fn first second
  (--system-do-compare-arithmetic! :>= first second)
))