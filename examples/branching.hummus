(def out (fn x
  (--system-do-io! :console-print x)
))

(def a true)

(def foo (fn "Yo"))

(out (if ((fn (foo)))
  "Hello" ; Hummus doesn't allow multiple statements, thus discouraging nesting
  (foo)))

(out "\n")

(if (foo)
  "Hello" ; Hummus doesn't allow multiple statements, thus discouraging nesting
  (foo))

; If you really want tho, you can do it

(if a
  ((fn
    (out "Hello")
    (out " ")
    (out "World")
  )))